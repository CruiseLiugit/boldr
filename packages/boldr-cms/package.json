{
  "name": "boldr-cms",
  "version": "0.7.0-alpha.0",
  "description": "universal react content management framework",
  "main": "boldr/boldrAPI/server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/strues/boldr.git"
  },
  "keywords": [
    "react",
    "universal",
    "javascript",
    "express",
    "webpack",
    "postgres",
    "node",
    "cms",
    "redux"
  ],
  "author": "Steven Truesdell <steven@strues.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/strues/boldr/issues"
  },
  "homepage": "https://github.com/strues/boldr#readme",
  "engines": {
    "node": ">=7.0.0",
    "npm": ">=3.0.0",
    "yarn": "^0.17.0"
  },
  "betterScripts": {
    "client": {
      "command": "babel-node ./tools/development",
      "env": {
        "DEBUG": "boldr:*",
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    },
    "api": {
      "command": "nodemon ./src/api/index.js",
      "env": {
        "DEBUG": "boldrAPI:*",
        "NODE_ENV": "development",
        "BABEL_ENV": "development"
      }
    }
  },
  "scripts": {
    "analyze": "babel-node tools/scripts/analyze",
    "api": "better-npm-run api",
    "apidoc": "apidoc -i src -o lib/apidoc && apidoc-markdown -p lib/apidoc -o docs/apidoc.md",
    "babel": "NODE_ENV=production babel --ignore node_modules",
    "build": "babel-node tools/scripts/build",
    "build:api": "NODE_ENV=production rimraf boldr/boldrAPI && npm run babel -- ./src/api -d ./boldr/boldrAPI/lib",
    "clean": "babel-node tools/scripts/clean",
    "client": "better-npm-run client",
    "dev": "npm-run-all -p api client",
    "development": "babel-node tools/development",
    "lint": "eslint src",
    "flow": "babel-node tools/scripts/flow",
    "flow:defs": "flow-typed install --overwrite",
    "flow:report": "flow-coverage-report -i 'src/**/*.js' -t html -t json -t text",
    "flow:remove": "babel-node tools/scripts/removeFlow && rimraf tools/scripts/removeFlow.js",
    "migrate": "POSTGRES_CONN_URI=postgres://postgres:password@localhost:5432/boldr knex --knexfile config/knexfile.js migrate:latest",
    "seed": "POSTGRES_CONN_URI=postgres://postgres:password@localhost:5432/boldr knex --knexfile config/knexfile.js seed:run",
    "start": "NODE_ENV=production node bin/boldr",
    "start:cms": "NODE_ENV=production pm2 start boldrCMS/server/index.js --name boldrSSR -e logs/boldrSSR-error.log",
    "test:ci": "NODE_ENV=test CI=true jest -w 2",
    "test": "NODE_ENV=test BABEL_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch=all"
  },

  "dependencies": {
    "app-root-dir": "1.0.2",
    "aws-sdk": "^2.7.20",
    "bcryptjs": "^2.4.0",
    "bluebird": "^3.4.7",
    "body-parser": "^1.15.2",
    "boldr-editor": "^0.7.0",
    "classnames": "^2.2.5",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "connect-redis": "^3.2.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.1",
    "date-fns": "^1.22.0",
    "dotenv": "2.0.0",
    "draft-js": "^0.9.1",
    "draft-js-export-html": "^0.5.2",
    "draft-js-import-html": "^0.3.2",
    "express": "4.14.0",
    "express-session": "^1.14.2",
    "express-validator": "^3.1.2",
    "express-winston": "^2.1.2",
    "helmet": "3.3.0",
    "hpp": "0.2.1",
    "http-proxy": "^1.16.2",
    "humps": "^2.0.0",
    "immutability-helper": "^2.1.0",
    "invariant": "^2.2.2",
    "jsonwebtoken": "^7.2.1",
    "knex": "^0.12.6",
    "lodash": "^4.17.4",
    "material-ui": "0.16.6",
    "method-override": "^2.3.7",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "multer-s3": "^2.5.0",
    "nodemailer": "^2.7.0",
    "normalizr": "3.0.0",
    "objection": "^0.6.2",
    "objection-find": "^0.3.0",
    "offline-plugin": "4.5.3",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "pg": "^6.1.2",
    "ramda": "^0.23.0",
    "react": "15.4.1",
    "react-addons-css-transition-group": "^15.4.1",
    "react-addons-shallow-compare": "^15.4.1",
    "react-addons-transition-group": "^15.4.1",
    "react-dom": "15.4.1",
    "react-helmet": "3.3.0",
    "react-redux": "5.0.1",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "react-router-scroll": "^0.4.1",
    "react-tap-event-plugin": "^2.0.1",
    "redial": "^0.5.0",
    "redis": "^2.6.3",
    "redux": "^3.6.0",
    "redux-form": "^6.4.3",
    "redux-form-material-ui": "^4.1.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "serialize-javascript": "1.3.0",
    "source-map-support": "0.4.8",
    "styled-components": "^1.2.1",
    "superagent": "^3.3.1",
    "uuid": "^3.0.1",
    "webfontloader": "^1.6.27",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.0",
    "babel-cli": "6.18.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-boldr": "0.7.2",
    "babel-preset-env": "1.1.4",
    "better-npm-run": "^0.0.13",
    "chokidar": "1.6.1",
    "css-loader": "0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "3.12.2",
    "eslint-config-boldr": "0.7.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "faker": "^3.1.0",
    "file-loader": "0.9.0",
    "flow-bin": "0.37.4",
    "flow-coverage-report": "0.2.0",
    "flow-remove-types": "1.1.2",
    "flow-typed": "2.0.0",
    "glob": "7.1.1",
    "happypack": "3.0.2",
    "html-webpack-plugin": "2.25.0",
    "jest-cli": "^18.1.0",
    "md5": "2.2.1",
    "nock": "^9.0.2",
    "node-notifier": "4.6.1",
    "node-sass": "^4.1.1",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.0",
    "postcss-cssnext": "^2.9.0",
    "postcss-loader": "^1.2.1",
    "react-addons-test-utils": "^15.4.1",
    "react-hot-loader": "3.0.0-beta.6",
    "redux-mock-store": "^1.2.1",
    "rimraf": "2.5.4",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "style-loader": "0.13.1",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.2",
    "webpack": "2.2.0-rc.3",
    "webpack-bundle-analyzer": "2.2.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.14.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4"
  }
}
