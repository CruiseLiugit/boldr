{
  "presets": [
    ["env", { "targets": { "node": true } }]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-es2015-destructuring",
    "transform-class-properties",
    "transform-flow-strip-types",
    ["module-resolver", {
      "root": ["./src/common"]
    }]
  ],
  "env": {
    "test": {
      "presets": [["boldr", { "es2015": { "modules": "commonjs" }}], "react"],
      "plugins": [
        ["module-resolver", {
          "root": ["./src/common"]
        }]
      ]
    }
  }
}
